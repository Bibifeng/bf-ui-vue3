{
	"name": "bf-ui-vue3",
	"version": "0.0.0",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"prettier": "prettier --write \"src/**/*.{ts,js,vue}\""
	},
	"dependencies": {
		"vue": "^3.0.0-rc.1",
		"vue-router": "4.0.0-beta.3"
	},
	"devDependencies": {
		"@vue/compiler-sfc": "^3.0.0-rc.1",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.1",
		"prettier": "^2.7.0",
		"sass": "1.26.10",
		"vite": "^1.0.0-rc.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{js,vue,ts.tsx}": [
			"npm run prettier",
			"git add"
		]
	}
}
